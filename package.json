{
  "name": "expensetracker",
  "version": "1.0.0",
  "description": "backend for expense tracker",
  "main": "server.js",
  "scripts": {
    "start": "node server",
    "lint": "eslint .",
    "server": "nodemon server",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"**/*.+(js|jsx|json|css|md)\"",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aEstebana/expenseTracker.git"
  },
  "author": "Esteban Acevedo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aEstebana/expenseTracker/issues"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "homepage": "https://github.com/aEstebana/expenseTracker#readme",
  "dependencies": {
    "colors": "^1.4.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongoose": "^5.12.4",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.2"
  }
}
